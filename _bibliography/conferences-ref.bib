	bdsk-url-1 = {http://dx.doi.org/10.1109/CDC.2011.6160752}}

@inproceedings{averaging-cdc11,
	abstract = {Averaging of fast switching systems is an effective technique used in many engineering applications. Practical stability and control design for a nonsmooth switched system can be inferred by analyzing the smooth averaged system. In this paper we overview the few formal approaches proposed in the literature to deal with the averaging of nonsmooth systems. The dithering, the phasor dynamics and the hybrid framework techniques are recast and compared by considering pulse-modulated switched linear systems as the common modeling platform.},
