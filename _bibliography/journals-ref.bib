	number = {12},
	pages = {6821--6835},
	pdf = {2014_TPE_compl_closed.pdf},
	title = {{A Complementarity Model for Closed-Loop Power Converters}},
	url = {http://dx.doi.org/10.1109/TPEL.2014.2306975},
