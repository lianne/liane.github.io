	note = {Impact Factor: 7.377},
	number = {1},
	pages = {35--46},
	pdf = {2017_TII_SemanticSmartGrid.pdf},
	rss-description = {

},
	title = {{A Semantic Middleware Supported Receding Horizon Optimal Power Flow in Energy Grids}},
	url = {http://ieeexplore.ieee.org/document/7822985/},
	volume = {14},
